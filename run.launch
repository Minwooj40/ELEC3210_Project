<launch>
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find slam)/rviz/project.rviz"/>
    <include file="$(find slam)/launch/slam.launch"/>
    <node pkg="image_detect" type="image_detect.py" name="image_detect" output="screen"/>
    <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop" output="screen">
    	<remap from="cmd_vel" to="/vrep/cmd_vel"/>
    </node>
    <node pkg="localization" type="localization.py" name="localization" output="screen"/>
    <node pkg="visual_servoing" type="visual_servoing.py" name="visual_servoing" output="screen"/>
</launch>
